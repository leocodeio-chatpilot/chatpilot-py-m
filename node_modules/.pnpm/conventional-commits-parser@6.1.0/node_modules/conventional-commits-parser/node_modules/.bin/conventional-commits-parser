#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/dist/cli/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/dist/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/dist/cli/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/dist/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules/conventional-commits-parser/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/conventional-commits-parser@6.1.0/node_modules:/home/leo/Desktop/leo-ext/self/production/chatpilot/chatpilot-py-m/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../dist/cli/index.js" "$@"
fi
